<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite parallel="tests" name="Suite">
	<listeners>
		<listener class-name="selenium.reusableabstractcomponents.Listeners"/>		
	</listeners>
  <test thread-count="5" name="Submit Order Tests">
    <classes>
      <class name="selenium.pageobjectmodel.Tests.SubmitOrderPOM"/>
    </classes>
  </test> 
  
   <test thread-count="5" name="Error Validation Tests">
    <classes>
      <class name="selenium.pageobjectmodel.Tests.ErrorValidation"/>
    </classes>
  </test>
</suite> 
